// Most of this was generated with 'dotnet new -t xunittest'
{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable"
  },
  "dependencies": {
    "System.Runtime.Serialization.Primitives": "4.3.0",

    // Needed to work around issue https://github.com/aspnet/EntityFramework/pull/6678
    "Microsoft.DotNet.InternalAbstractions": "1.0.0",

    // XUnit packages
    "xunit": "2.2.0-beta5-build3474",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",

    // This can be found (even in a different directory)
    // thanks to our global.json
    "TaskTracker": {
      "target": "project",
      "version": "1.0.0"
    }
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      }
    }
  },

  // Specify the test runner
  "testRunner": "xunit"
}
